{
  "globals": {
    "window": "readonly",
    "document": "readonly",
    "Intl": "readonly",
    "console": "readonly"
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "env": {
    "es6": true
  },
  "rules": {
    "no-cond-assign": 0,
    "no-prototype-builtins": 0
  }
}
